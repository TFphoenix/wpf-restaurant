<Window x:Class="Restaurant.StartupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Restaurant.Views"
        mc:Ignorable="d"
        Title="DORU'S Restaurant - Startup Page"
        Height="800" Width="600"
        ResizeMode="NoResize"
        Icon="../Resources/logo_min.png"
        Background="#F7F7F7">

  <Grid>
    <views:SignInView x:Name="SignInView" Visibility="Visible"/>
    <views:SignUpView x:Name="SignUpView" Visibility="Hidden"/>
  </Grid>
</Window>
